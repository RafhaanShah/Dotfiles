{
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "off",
      "other": "off",
      "strings": "off"
    },
    "editor.wordWrap": "on"
  },
  "debug.console.fontFamily": "MesloLGM Nerd Font, Consolas, monospace",
  "debug.console.fontSize": 12,
  "debug.saveBeforeStart": "nonUntitledEditorsInActiveGroup",
  "diffEditor.ignoreTrimWhitespace": false,
  "docker.extension.enableComposeLanguageServer": true,
  "editor.accessibilitySupport": "off",
  "editor.fontFamily": "JetBrainsMono NF, JetBrains Mono, Consolas, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.lightbulb.enabled": "on",
  "editor.wordWrap": "on",
  "explorer.autoReveal": false,
  "files.autoSave": "afterDelay",
  "files.exclude": {
    ".bemol": true,
    ".viceroy": true,
    "**/.classpath": true,
    "**/.DS_Store": true,
    "**/.factorypath": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/Thumbs.db": true,
    "apollo-overrides": true,
    "build": false,
    "env": true,
    "packageInfo*": true,
    "release-info": true,
    "src/*/build": false
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.watcherExclude": {
    "**/.bootstrap/**": true,
    "**/.git/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/.idea/**": true,
    "**/.repo/**": true,
    "**/build/**": true,
    "**/node_modules/*/**": true,
    "**/out/**": true
  },
  "git.autofetch": true,
  "git.autofetchPeriod": 600,
  "git.blame.editorDecoration.enabled": false,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "github.copilot.enable": {
    "*": false
  },
  "go.useLanguageServer": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jupyter.askForKernelRestart": false,
  "python.analysis.typeCheckingMode": "standard",
  "redhat.telemetry.enabled": false,
  "search.collapseResults": "auto",
  "search.exclude": {
    "**/.bootstrap/**": true,
    "**/.git/**": true,
    "**/.idea/**": true,
    "**/.repo/**": true,
    "**/*.code-search": true,
    "**/bower_components": true,
    "**/node_modules": true,
    "**/out/**": true,
    "build": true,
    "env": true,
    "src/*/build": true
  },
  "syncSettings.ignoredSettings": [
    "[blueprint]",
    "[c]",
    "[cpp]",
    "[cuda-cpp]",
    "[go]",
    "[hack]",
    "[home-assistant]",
    "[java]",
    "[javascript]",
    "[json]",
    "[jsonc]",
    "[kotlin]",
    "[objective-c]",
    "[objective-cpp]",
    "[ocaml]",
    "[proto]",
    "[python]",
    "[rust]",
    "[starlark-skycastle]",
    "[starlark]",
    "[swift]",
    "[tcc-cpp]",
    "[typescript]",
    "[typescriptreact]",
    "[yaml]",
    "devmate.clickableSparkleDecorator",
    "extensions.experimental.deferredStartupFinishedActivation",
    "fb.ms-remote-connections.odtypes",
    "meta.global.history.enabled",
    "remote.defaultExtensionsIfInstalledLocally",
    "remote.SSH.remotePlatform",
    "settingsSync.ignoredExtensions",
    "settingsSync.ignoredSettings",
    "syncing.excludedExtensions",
    "syncing.excludedSettings",
    "terminal.integrated.systemd.shouldUse",
    "wasabi.interactiveDevelopment.disableIntroMessage",
    "wasabi.interactiveDevelopment.introMessageNumTimesShown",
    "yaml.customTags",
  ],
  "syncSettings.keybindingsPerPlatform": true,
  "syncSettings.resources": [
    "keybindings",
    "settings",
  ],
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.fontFamily": "MesloLGM Nerd Font, Consolas, monospace",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.fontWeight": "100",
  "terminal.integrated.lineHeight": 0,
  "terminal.integrated.sendKeybindingsToShell": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.menuBarVisibility": "classic",
  "window.restoreWindows": "none",
  "workbench.colorTheme": "Monokai",
  "workbench.commandPalette.experimental.enableNaturalLanguageSearch": false,
  "workbench.editor.enablePreview": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.enableNaturalLanguageSearch": false,
}
